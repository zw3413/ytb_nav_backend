{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "FastAPI Debug (uvicorn with reload)",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "main:app",
          "--host", "0.0.0.0",
          "--port", "8000"
        ],
        "jinja": true,
        "justMyCode": true,
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        }
      }
    ]
  }
  